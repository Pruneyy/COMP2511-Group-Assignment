<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.layout.Pane?>
<BorderPane minHeight="350" minWidth="500" fx:id="game" fx:controller="sample.Controller"
			xmlns:fx="http://javafx.com/fxml">
	<center>
		<Pane fx:id="stackPane">
			<BorderPane.margin>
				<Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
			</BorderPane.margin>
			<GridPane fx:id="gameBoard" gridLinesVisible="true" hgap="2" vgap="2">
			</GridPane>
		</Pane>
	</center>
	<right>
		<VBox>
			<BorderPane.margin>
	             <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
	        </BorderPane.margin>
		    <Button fx:id="start" id="start" text="PLAY"
		    	onAction="#handleButtonPress"/>
		</VBox>
	</right>
</BorderPane>
